{
  "name": "react-native-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ios:clean": "xcodebuild clean -workspace ReactNativeBoilerplate.xcworkspace -scheme ReactNativeBoilerplate -configuration Debug",
    "ios:pod": "cd ios && pod install && cd ..",
    "postinstall": "patch-package",
    "android:apk": "cd android && ./gradlew assembleRelease && cd ..",
    "android:clean": "cd android && gradle clean && cd ..",
    "android:keystore": "keytool -genkey -v -keystore android/app/development-release.keystore -alias key0 -keyalg RSA -keysize 2048 -validity 10000"
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.11.1",
