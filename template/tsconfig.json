{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "allowJs": false,
    "jsx": "react-native",
    "noImplicitAny": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "ESNext",
    "lib": [
      "ES2017"
    ],
    "paths": {
      "$assets/*": [
        "app/assets/*"
      ],
      "$components/*": [
        "app/components/*"
      ],
      "$constants/*": [
        "app/constants/*"
      ],
      "$context/*": [
        "app/context/*"
      ],
      "$dev/*": [
        "app/dev/*"
      ],
      "$dto/*": [
        "app/dto/*"
      ],
      "$helpers/*": [
        "app/helpers/*"
      ],
      "$hooks/*": [
        "app/hooks/*"
      ],
      "$locales/*": [
        "app/locales/*"
      ],
      "$mock/*": [
        "app/mock/*"
      ],
      "$navigation/*": [
        "app/navigation/*"
      ],
      "$screens/*": [
        "app/screens/*"
      ],
      "$styles/*": [
        "app/styles/*"
      ],
      "$types/*": [
        "app/types/*"
      ],
      "$utils/*": [
        "app/utils/*"
      ]
    }
  }
}